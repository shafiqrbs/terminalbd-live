    %}
    <link rel="stylesheet" href="{{ asset_url }}"/>
    {% endstylesheets %}
    <style>
        .grid-products {
            grid-template-columns: repeat({{ globalOption.ecommerceConfig.mobileProductColumn }}, calc(100%/{{ globalOption.ecommerceConfig.mobileProductColumn }}));
            display: grid;
        }
        .feature-grid-products {
            grid-template-columns: repeat({{ globalOption.ecommerceConfig.mobileFeatureColumn }}, calc(100%/{{ globalOption.ecommerceConfig.mobileFeatureColumn }}));
            display: grid;
        }
    </style>
    <!-- web-fonts -->
    <link href='//fonts.googleapis.com/css?family=Abril+Fatface' rel='stylesheet' type='text/css'>
    <link href='//fonts.googleapis.com/css?family=Open+Sans:400,300,300italic,400italic,600,600italic,700,700italic,800,800italic' rel='stylesheet' type='text/css'>
