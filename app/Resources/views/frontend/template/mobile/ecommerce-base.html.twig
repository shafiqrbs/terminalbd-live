<body>
{% block cssBlock %}{% endblock %}
<div class="body-back siteBgColor">
    <div class="masthead pdng-stn1">
        <div class="phone-box wrap push" id="home">
            <div class="menu-notify sticky">
{#                <div class="bar-left">
                    <a href="#mm-menu" class="main-menu"><i class="fa fa-navicon"></i></a>
                </div>
                <div class="bar-mid">
                    <h5 class="pro-link"><a class="site-name" href="{{ path(globalOption.subDomain ~ '_ecommercesubdomain') }}">{% if globalOption.templateCustomize.mobileShowLogo == 1 %}<img src="{{ image(globalOption.templateCustomize.webPath('logo')).resize(120,30) }}"> {% else %}{{ globalOption.mobileName }}{% endif %}</a></h5>

                </div>
                <div class="bar-right">
                   #}{# <a href="javascript:" style="position: relative"  class="showCartItem bar-icon"><i class="fa fa-shopping-bag"></i>
                    <span id="item-qnt" class="totalItem">{{ cart.contents|length }}</span>
                    </a>#}{#
                    <div class="input-group">
                    <span id="logged-in">
                         {% if app.user is not empty %}
                             {% set name = app.user.profile.name|split(" ") %}
                             <a class="bar-icon" href="{{ path(globalOption.subDomain ~ '_domain_authentication') }}"><i class="fa fa-home"></i></a>&nbsp;&nbsp;<a href="javascript:" class="logout bar-icon" data-action="/logout"> <i class="fa fa-sign-out"></i></a>
                             {% else %}
                             <a href="#login-modal" class="popup-top-anim bar-icon" ><i class="fa fa-user"></i></a>
                         {% endif %}
                    </span>
                    {% if globalOption.ecommerceConfig.uploadFile == 1 %}
                    <a href="javascript:" class="bar-icon showCartItem" ><i class="fa fa-camera"></i></a>
                    {% endif %}
                    <a href="javascript:" class="bar-icon bar-search" ><i class="fa fa-search"></i></a>
                    </div>
                </div>
                <div class="clearfix"></div>#}
                <div id="search-box" style="display: block">
                    <form  action="{{ path(globalOption.subDomain ~ '_webservice_product') }}"  method="get" >
                        <div class="input-group" style="margin-top: 6px">
                            <span class="input-group-btn"> <a href="#mm-menu" class="btn btn-transparent bar-search main-menu"><i class="fa fa-navicon"></i></a></span>

                            <div style="width: 100%; position: relative">
                            <button  style="position: absolute; top: 5px; right: 5px;z-index: 999"><i class="fa fa-search"></i></button>
                            <input type="text" name="webName" required="required" id="webName" class="form-control" value="{{ searchForm['webName'] is defined ? searchForm['webName'] :'' }}" placeholder="Find a desired products">
                            </div>
                            <span class="input-group-btn">

                             <button type="button"  id="sideMenu" class="btn btn-transparent sideviewtoggle" ><i class="fa fa-ellipsis-v"></i></button>
                            </span>
                        </div>
                    </form>
                </div>
            </div>
            <!-- banner -->
            {% block body %}{% endblock %}
            <!--/footer-->
            <div class="w3agile alt-mobile-footer">
                {% if globalOption.templateCustomize.mobileFooter == 'contact' %}
                    <div class="footer-bg" id="mobile-footer">
                        <ul class="footer-icon">

                            <li>
                                <a href="tel://88{{ globalOption.hotline }}" class="hvr-radial-out" data-title="Phone">
                                    <i area-hidden=true class="fa fa-phone"></i>
                                </a>
                            </li>
                            <li>
                                <a href="#login-modal" class="hvr-radial-out sign-in popup-top-anim" data-title="">
                                    <i class="fa fa-comment"></i>
                                </a>
                            </li>
                            <li>
                                <a href="javascript:" class="scrollToTop" data-title="Top up">
                                    <i class="fa fa-hand-o-up"></i>
                                </a>
                            </li>
                            <li>
                                <a href="#email-modal" class="hvr-radial-out sign-in popup-top-anim">
                                    <i class="fa fa-envelope"></i>
                                </a>
                            </li>
                            <li>
                                <a class="last hvr-radial-out"
                                   href="{{ path(globalOption.subDomain ~ '_webservice_contact') }}" data-title="">
                                    <i class="fa fa-map-marker"></i>
                                </a>
                            </li>
                        </ul>
                    </div>
                {% elseif globalOption.templateCustomize.mobileFooter == 'cart' %}
                    <div class="footer-bg" id="mobile-footer">
                        <div class="sidebar-footer">
                            <a class="btn btn-warning text-center col-xs-6 col-md-6 mobile-order" href="tel:+88{{ globalOption.templateCustomize.contactNo }}"><i class="fa fa-phone"></i> ORDER BY PHONE</a>
                            <a href="javascript:" class="btn cart-info col-xs-4 col-md-4 showCartItem"><span class="totalItem footer-item">{{ cart.contents|length }}</span> | {{ globalOption.ecommerceConfig.currency }} <span class="totalAmount footer-item-tk">{{ cart.total|number_format(2) }}</span></a>
                            <a href="javascript:"  class="btn btn-info col-xs-2 col-md-2 showCartItem btn-shopping-cart">
                                <span class="fa fa-shopping-bag" aria-hidden="true"></span>
                            </a>
                        </div>
                    </div>
                {% elseif globalOption.templateCustomize.mobileFooter == 'footer' %}
                    <div class="container">
                        <div class="copyright-footer">
                            Â© {{ "now"|date('Y') }} <a target="_blank" href="http://www.terminalbd.com">Terminalbd</a> All Rights Reserved.
                        </div>
                    </div>
                {% endif %}
            </div>
        </div>

        <!--/footer-->
    </div>
</div>

{#{% render controller("FrontendBundle:TemplateWidget:modalLogin",{'globalOption':globalOption.id}) %}#}
<div id="product-modal" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myLargeModalLabel" >
    <div class="modal-dialog modal-lg" role="document">
        <div class="product-modal-content"></div>
    </div>
</div>
<div id="sideMenuContainer">
    {% if app.user is not empty %}
        <h2 class="text-center">{{ app.user.profile.name }}</h2>
        {% set name = app.user.profile.name|split(" ") %}
        <a class="bar-icon" title="My profile" href="{{ path(globalOption.subDomain ~ '_domain_authentication') }}"><i class="fa fa-user"></i></a>
        <a href="{{ path(globalOption.subDomain ~ '_webservice_buyer_order') }}" title="My Order" class="" ><i class="fa fa-shopping-cart"></i></a>
        <a href="javascript:" class="logout" title="Logout" data-action="/logout"> <i class="fa fa-sign-out"></i></a>
    {% else %}
        <h2 class="text-center">{{ globalOption.name }}</h2>
        <a href="javascript:" title="Login" class="showCartItem" ><i class="fa fa-user"></i></a>
    {% endif %}
    {% if globalOption.ecommerceConfig.uploadFile == 1 %}
    <a href="{{ path(globalOption.subDomain ~ '_webservice_product_cart_details') }}" title="Upload Order"  ><i class="fa fa-camera"></i></a>
    {% endif %}
    <a href="{{ path(globalOption.subDomain ~ '_webservice_product_cart_details') }}" title="Checkout"  ><i class="fa fa-shopping-cart"></i></a>
</div>
<div id="email-confirm" style="display:none; text-align: left!important;">Thank you for your interest in {{ globalOption.domain }}. Your email has been sent successfully to our customer service department.</div>
<div id="email-invalid" style="display:none; text-align: left!important;">Dear Customer, All ready your email/mobile has been send. Thank you for your interest in {{ globalOption.domain }}.</div>
<div id="product-confirm" style="display:none; text-align: left!important;">Thank you for your interest in {{ globalOption.domain }}. Your order has been sent successfully to our customer service department.</div>
{#<div id="mobile-confirm" style="display:none; text-align: left!important;">Thank you for your interest in {{ globalOption.domain }}.<br/><span id="mobile-validate"></span></div>#}
<div id="mobile-confirm" style="display:none; text-align: left!important;">
<span id="mobile-validate"></span></div>
{% if globalOption.templateCustomize.sidebarTooltip == 1 %}
    <div class="side-toolbar" style=""></div>
{% endif %}

{% javascripts
'assets/template/mobile/js/jquery-2.1.4.min.js'
