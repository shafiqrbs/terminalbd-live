%}
<script type="text/javascript" src="{{ asset_url }}"></script>
{% endjavascripts %}
<script>  subdomain = "{{ globalOption.subDomain }}"; </script>
{% block javascriptBlock %}{% endblock %}
<script type="text/javascript">
    $(document).ready(function () {
        {% block documentready %}{% endblock %}
        $(window).on('load', function() {});

        $(document).on( "click", ".bar-search", function(e){
            $('#search-box').slideToggle('2000');
        });

        $('#sideMenu').sideToggle({
            moving: '#sideMenuContainer',
            direction: 'right'
        });
        {% if globalOption.templateCustomize.sidebarTooltip == 1 %}

        $('.side-toolbar').vSidebar({
            'items': [
                {% render controller("FrontendBundle:TemplateWidget:sidebar",{'globalOption':globalOption.id}) %}
            ],
            'position': 'right bottom'
        });
        {% endif %}
        if( /Android|webOS|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) ) {

        }else if( /iPhone|iPad|iPod/i.test(navigator.userAgent) ) {
            $(".bs-callout").css('margin-bottom','60px');
        }
        $(document).on( "click", ".menuLink", function(e){
           url = $(this).attr('data-action');
            window.location = url;
        });
    })
</script>
{% render controller("FrontendBundle:EcommerceWidget:mobileMenu",{'globalOption': globalOption.id}) %}
{% if globalOption.templateCustomize.fbMessenger is not empty  %}
    <div class="fb-customerchat"
         page_id="{{ globalOption.templateCustomize.fbMessenger }}">
    </div>
{% endif %}

{% if globalOption.templateCustomize.tawk is not empty  %}
    <script type="text/javascript">
        var Tawk_API=Tawk_API||{}, Tawk_LoadStart=new Date();
        (function(){
            var s1=document.createElement("script"),s0=document.getElementsByTagName("script")[0];
            s1.async=true;
            s1.src='https://embed.tawk.to/{{ globalOption.templateCustomize.tawk }}/default';
            s1.charset='UTF-8';
            s1.setAttribute('crossorigin','*');
            s0.parentNode.insertBefore(s1,s0);
        })();
    </script>
{% endif %}

{% if globalOption.templateCustomize.liveChat is not empty  %}
    <script type="text/javascript">function add_chatinline(){var hccid={{ globalOption.templateCustomize.liveChat }};var nt=document.createElement("script");nt.async=true;nt.src="https://mylivechat.com/chatinline.aspx?hccid="+hccid;var ct=document.getElementsByTagName("script")[0];ct.parentNode.insertBefore(nt,ct);}
        add_chatinline(); </script>
{% endif %}

{% if globalOption.templateCustomize.powr is not empty  %}
    <div class="powr-chat" id="{{ globalOption.templateCustomize.powr }}"></div><script src="https://www.powr.io/powr.js?platform=html"></script>
{% endif %}
{% if globalOption.templateCustomize.googleAnalytic is not empty  %}
    <script async src="https://www.googletagmanager.com/gtag/js?id={{ globalOption.templateCustomize.googleAnalytic }}"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag(){dataLayer.push(arguments);}
        gtag('js', new Date());
        gtag('config', '{{ globalOption.templateCustomize.googleAnalytic }}');
    </script>
{% endif %}
{% if globalOption.templateCustomize.facebookPixel is not empty  %}
    <!-- Facebook Pixel Code -->
    <script>
        !function(f,b,e,v,n,t,s)
        {if(f.fbq)return;n=f.fbq=function(){n.callMethod?
            n.callMethod.apply(n,arguments):n.queue.push(arguments)};
            if(!f._fbq)f._fbq=n;n.push=n;n.loaded=!0;n.version='2.0';
            n.queue=[];t=b.createElement(e);t.async=!0;
            t.src=v;s=b.getElementsByTagName(e)[0];
            s.parentNode.insertBefore(t,s)}(window,document,'script',
            'https://connect.facebook.net/en_US/fbevents.js');
        fbq('init', '{{ globalOption.templateCustomize.facebookPixel }}');
        fbq('track', 'PageView');
    </script>
    <noscript>
        <img height="1" width="1"
             src="https://www.facebook.com/tr?id={{ globalOption.templateCustomize.facebookPixel }}&ev=PageView
&noscript=1"/>
    </noscript>
    <!-- End Facebook Pixel Code -->
{% endif %}
<style>
    .mm-menu.mm-current {
        display: block!important;
    }

    .modal-dialog {
        top: 0%;
    }
    #product-modal{
        background:#DDDDDD;
    }
    .product-modal-content{
        background: #FFFFff;
    }
    .modal-title {
        margin: 0;
        line-height: 16px;
        width: 93%;
        font-weight: bold;
        text-align: left;
    }
    .modal-close-btn{
        position: absolute;
        right: 0;
        top: 15px;
    }
    .mfp-wrap{
        z-index: 9999;
    }

</style>
